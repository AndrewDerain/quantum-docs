{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": ".",
        "dest": "",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "quantum/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "show_latex": true,
      "ms.service": "azure-quantum",
      "ms.devlang": "qsharp",
      "ms.topic": "conceptual",
      "searchScope": ["quantum", "q#", "qsharp"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/quantum-docs",
      "feedback_product_url": "https://github.com/MicrosoftDocs/quantum-docs/issues",
      "titleSuffix": "Azure Quantum ",
      "breadcrumb_path": "/quantum/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "zone_pivot_group_filename": "/quantum/zone-pivot-groups.json"
    },
    "fileMetadata": {
      "latex_macros": {
        "**/**.md": {
          "ket": ["\\left| #1\\right\\rangle", 1],
          "bra": ["\\left\\langle #1\\right|", 1],
          "braket": [ "\\left\\langle #1 \\right\\rangle", 1 ],
          "id": "\\mathbb{1}",
          "boldone": "\\mathbf{1}",
          "expect": "\\mathbb{E}",
          "variance": "\\operatorname{Var}",
          "defeq": "\\mathrel{:=}",
          "dd": "\\mathrm{d}"
        }
      }
    },
    "template": [],
    "dest": "quantum-docs"
  }
}
